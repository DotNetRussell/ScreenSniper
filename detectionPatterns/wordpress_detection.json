{
  "name": "WordPress Detection",
  "conditions": {
    "type": "AND",
    "values": [
      "wordpress",
      {
        "type": "OR",
        "values": ["wp-content", "wp-admin"]
      }
    ]
  },
  "negative_conditions": ["allow", "disallow", "site-map"],
  "meta_tags": [
    "Technology: WordPress",
    "SecurityRisk: Check for Known Vulnerabilities"
  ]
}